{
    "name": "jerrkill\/twgetter",
    "description": "A tweets getter",
    "license": "MIT",
    "authors": [
        {
            "name": "jerrkill",
            "email": "jerrkill123@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "~6.3",
        "campo/random-user-agent": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Jerrkill\\Twgetter\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jerrkill\\Twgetter\\TwgetterServiceProvider"
            ],
            "aliases": {
                "Twgetter": "Jerrkill\\Twgetter\\Facades\\Twgetter"
            }
        }
    }
}
